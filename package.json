{
  "name": "dlife",
  "version": "0.0.1",
  "description": "Blood Glucose Simulation Library",
  "main": "demo/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "test": "nyc mocha",
    "testsnyk": "snyk test",
    "testhtml": "nyc --reporter=html --reporter=text mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint lib",
    "flow": "flow",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel test lint",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuddster/dlife.git"
  },
  "keywords": [
    "bgl",
    "simulation"
  ],
  "files": [
    "demo",
    "lib"
  ],
  "contributors": [
    "Scott Elmer <fudd_dlife@elmerfamily.com> (http://elmerfamily.com/)",
    "Evelyn Elmer <eve_dlife@elmerfamily.com> (http://elmerfamily.com/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuddster/dlife/issues"
  },
  "homepage": "https://github.com/fuddster/dlife#readme",
  "engines": {
    "node": ">=12.16.1"
  },
  "dependencies": {
    "d3": "^5.15.0",
    "dom-parser": "^0.1.6",
    "jsdom": "^16.2.1",
    "jsdom-global": "^3.0.2",
    "json-loader": "0.5.7",
    "npm": "^6.14.4",
    "npm-check-updates": "4.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sinon": "^9.0.1",
    "sweetalert2": "^9.10.6",
    "unique-random-array": "2.0.0",
    "webpack": "5.5.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "4.2.0",
    "codecov": "^3.6.5",
    "commitizen": "4.0.3",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "flow-bin": "^0.121.0",
    "ghooks": "2.0.4",
    "mocha": "^7.1.1",
    "mocha-jsdom": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "opt-cli": "1.6.0",
    "rimraf": "3.0.2",
    "semantic-release": "^17.0.4",
    "validate-commit-msg": "2.14.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 30,
    "function": 60,
    "lines": 50,
    "statements": 50,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "lib"
    ],
    "sourceMap": false
  },
  "snyk": true
}
