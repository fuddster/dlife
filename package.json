{
  "name": "dlife",
  "version": "0.0.2",
  "description": "Blood Glucose Simulation Library",
  "main": "demo/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "test": "nyc mocha",
    "testhtml": "nyc --reporter=html --reporter=text mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint lib",
    "flow": "flow",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel test lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuddster/dlife.git"
  },
  "keywords": [
    "bgl",
    "simulation"
  ],
  "files": [
    "demo",
    "lib"
  ],
  "contributors": [
    "Scott Elmer <fudd_dlife@elmerfamily.com> (http://elmerfamily.com/)",
    "Evelyn Elmer <eve_dlife@elmerfamily.com> (http://elmerfamily.com/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuddster/dlife/issues"
  },
  "homepage": "https://github.com/fuddster/dlife#readme",
    "engines" : { 
    "node" : ">=8.16.0" 
  },
  "dependencies": {
    "dom-parser": "^0.1.6",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "npm": "^6.4.1",
    "npm-check-updates": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "sinon": "^7.3.0",
    "sweetalert2": "^8.12.1",
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "^3.0.4",
    "commitizen": "4.0.3",
    "d3": "^5.0.0",
    "eslint": "^6.0.0",
    "eslint-config-google": "^0.14.0",
    "flow-bin": "^0.108.0",
    "ghooks": "2.0.4",
    "jsdom-global": "^3.0.2",
    "json-loader": "0.5.7",
    "opt-cli": "1.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "2.7.0",
    "semantic-release": "^15.13.16",
    "validate-commit-msg": "2.14.0",
    "webpack": "4.41.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 30,
    "function": 60,
    "lines": 50,
    "statements": 50,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "lib"
    ],
    "sourceMap": false
  }
}
